{
  "name": "examples-minimal-react-server",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts"
    }
  },
  "imports": {
    "#*": {
      "typescript": "./src/*",
      "default": "./dist/*"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx watch -C typescript src/index.ts",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" src",
    "start": "node dist/src/index.js",
    "test": "vitest run"
  },
  "dependencies": {
    "@trpc/server": "npm:@trpc/server@next",
    "cors": "^2.8.5",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/node": "^20.10.0",
    "eslint": "^8.57.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.4"
  }
}
